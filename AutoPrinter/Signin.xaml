<Window x:Class="AutoPrinter.Signin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutoPrinter"
        mc:Ignorable="d"
        Title="YummyOnline自助打印" Height="353" Width="443">
	<Grid>
		<Grid Margin="50,50,50,30">
			<TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" Text="工号" VerticalAlignment="Top"/>
			<TextBox x:Name="textBoxSigninName" Height="23" Margin="10,30,10,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top"/>
			<TextBlock x:Name="textBlock1" Margin="10,58,0,0" TextWrapping="Wrap" Text="密码" VerticalAlignment="Top" HorizontalAlignment="Left" Width="24"/>
			<PasswordBox x:Name="passwordBox" Margin="10,78,10,0" VerticalAlignment="Top" Height="23"/>
			<Button x:Name="buttonSignin" Content="登录" Margin="10,106,10,0" VerticalAlignment="Top" Click="buttonSignin_Click" Height="23"/>
			
		</Grid>
		<TextBlock x:Name="textBlockVersion" Margin="10,0,10,10" TextWrapping="Wrap" Text="Version" TextAlignment="Right" Height="15" VerticalAlignment="Bottom"/>


	</Grid>
</Window>
